{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@picocss/pico": "^2.1.1",
    "nuxt": "^4.3.0",
    "vue": "^3.5.27",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.13.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "vite-plugin-vuetify": "^2.1.3",
    "vuetify": "^3.11.8"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue}": "npx eslint --fix",
    "*.{js,ts,tsx,vue,css,scss,md,json,yaml}": "npx prettier --write --ignore-unknown"
  }
}
